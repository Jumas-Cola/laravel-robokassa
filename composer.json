{
  "name": "jumas-cola/laravelrobokassa",
  "description": "Package for Laravel for working with a robokassa",
  "authors": [
    {
      "name": "Davidnadejdin",
      "email": "Davidnadejdin@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Davidnadejdin\\LaravelRobokassa\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "laravel/framework": "~5.5 || ~6.0 || ~7.0 || ~8.0",
    "jumas-cola/robokassa": "dev-master"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Davidnadejdin\\LaravelRobokassa\\RobokassaServiceProvider"
      ]
    }
  },
  "repositories": [
     {
         "url": "https://github.com/jumas-cola/robokassa",
         "type": "git"
     }
  ],
  "license": "Apache-2.0"
}
